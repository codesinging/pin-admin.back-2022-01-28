{
    "name": "codesinging/pin-admin",
    "description": "A Laravel package to rapidly build administrative applications",
    "license": "MIT",
    "authors": [
        {
            "name": "codesinging",
            "email": "codesinging@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.80"
    },
    "autoload": {
        "psr-4": {
            "CodeSinging\\PinAdmin\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeSinging\\PinAdmin\\PinAdminServiceProvider"
            ],
            "aliases": {
                "Admin": "CodeSinging\\PinAdmin\\Admin"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.24"
    }
}
